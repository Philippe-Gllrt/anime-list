<h1>Animes#index</h1>

<% @anime.each do |element|%>

  <p> <%= element.title %> </p>
  <p> <%= element.season  %> </p>
  <p> <%= element.synopsis %> </p>
  <p> <%= element.comment %> </p>
  <p> <%= element.link %> </p>
  <img src="<%=element.image_url%>" alt=<%="#{element.title} Illustration picture"%>>

<% end %>
